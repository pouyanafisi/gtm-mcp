{
  "name": "gtm-mcp",
  "version": "1.0.0",
  "description": "MCP server for Google Tag Manager",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gtm-mcp": "dist/index.js",
    "gtm-mcp-auth": "dist/auth.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "type-check": "tsc --noEmit",
    "auth": "tsx src/auth.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-tag-manager",
    "gtm",
    "analytics"
  ],
  "author": "Pouya Nafisi",
  "homepage": "https://github.com/pouyanafisi/gtm-mcp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pouyanafisi/gtm-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/pouyanafisi/gtm-mcp/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "googleapis": "^144.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
